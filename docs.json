          }
        ]
      },
      {
        "language": "en",
        "tabs": [
          {
            "tab": "Guide",
            "groups": [
              {
                "group": "Getting Started",
                "pages": [
                  "en/start/introduction",
                  "en/start/model-overview",
                  "en/start/quick-start"
                ]
              }
            ]
          },
          {
            "tab": "API Docs",
            "groups": [
              {
                "group": "API Guide",
                "pages": [
                  "en/api/introduction",
                  "en/api/rate-limit"
                ]
              },
              {
                "group": "Model API",
                "pages": [
                  "en/api/model-api/get-plants",
                  "en/api/model-api/post-plants",
                  "en/api/model-api/delete-plant"
                ]
              }
            ]
          },
          {
            "tab": "Examples",
            "groups": [
              {
                "group": "IDE Integration",
                "pages": [
                  "en/develop/claude",
                  "en/develop/claude-for-ide"
                ]
              }
            ]
          }
        ]
      }
    ]
