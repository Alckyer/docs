{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "aspen",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#0052d9",
    "light": "#618dff",
    "dark": "#002a7c"
  },
  "api": {
