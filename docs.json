{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "aspen",
  "name": "Mint Starter Kit",
  "i18n": {
    "default": "zh",
    "languages": [
      { "locale": "zh", "name": "简体中文" },
      { "locale": "en", "name": "English" }
    ]
  },
  "colors": {
    "primary": "#0052d9",
    "light": "#618dff",
    "dark": "#002a7c"
  },
  "api": {
    "playground": {
      "mode": "show"
    }
  },
  "openapi": "api/openapi.json",
  "css": "/custom.css",
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "使用指南",
        "groups": [
          {
            "group": "开始使用",
            "pages": [
              "start/introduction",
              "start/model-overview",
              "start/quick-start"
            ]
          }
        ]
      },
      {
        "tab": "API文档",
        "groups": [
          {
            "group": "API指引",
            "pages": [
              "api/introduction",
              "api/rate-limit"
            ]
          },
          {
            "group": "模型API",
            "pages": [
              "api/model-api/get-plants",
              "api/model-api/post-plants",
              "api/model-api/delete-plant"
            ]
          }
        ]
      },
      {
        "tab": "场景示例",
        "groups": [
          {
            "group": "开发工具接入",
            "pages": [
              "develop/claude",
              "develop/claude-for-ide"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hi@mintlify.com"
      }
    ],
    "languageToggle": true
  },
  "contextual": {
    "options": [
     "copy",
     "view"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}
