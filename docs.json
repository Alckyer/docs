                "pages": [
                  "start/introduction",
                  "start/model-overview",
                  "start/quick-start"
                ]
              }
            ]
          },
          {
            "tab": "API文档",
            "groups": [
              {
                "group": "API指引",
                "pages": [
                  "api/introduction",
                  "api/rate-limit"
                ]
              },
              {
                "group": "模型API",
                "pages": [
                  "api/model-api/get-plants",
                  "api/model-api/post-plants",
                  "api/model-api/delete-plant"
                ]
              }
            ]
          },
          {
            "tab": "场景示例",
            "groups": [
              {
                "group": "开发工具接入",
                "pages": [
                  "develop/claude",
                  "develop/claude-for-ide"
